              <a><%= link_to "Editar Perfil", edit_user_registration_path, :class => 'btn btn-outline-primary'%></a>
              <a><%= link_to "Salir", destroy_user_session_path, method: :delete, :class => 'btn btn-outline-primary'%></a>        
        </ul>
      </div>
    </nav>
    
