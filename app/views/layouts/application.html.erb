            </li>
          </ul>
        </div>
    </div>
    <br>
    <br>
    <div class="app-content content container-fluid">
      <div class="content-wrapper">
          <div class="content-body"><!-- stats -->
            <div class="row">
           
            <% if notice %>
            <p class="alert alert-success"><%= notice %></p>
            <% end %>
            <% if alert %>
            <p class="alert alert-danger"><%= alert %></p>
            <% end %>
            <%= yield %>
            <br>
            <br>
            <br>
            </div>
          </div>
      </div>
    </div>
    
