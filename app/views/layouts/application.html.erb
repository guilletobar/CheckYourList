        </div>
        <!-- / main menu header-->
        <!-- main menu content-->
        <div class="main-menu-content">
          <ul id="main-menu-navigation" data-menu="menu-navigation" class="navigation navigation-main ">
            <li class=" nav-item"><a href="/../../welcome/index"><i class="icon-home3"></i><span data-i18n="nav.dash.main" class="menu-title">Dashboard</span><span class="tag tag tag-primary tag-pill float-xs-right mr-2">2</span></a>
            </li>
