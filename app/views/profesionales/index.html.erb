            <div class="card">
                <div class="card-body">
                  <div class="col-lg-10 col-md-10 col-sm-9 col-xs-12">
         <p id="notice"><%= notice %></p>

<h1>Profesionales</h1>

<table>
  <thead>
    <tr>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Segundo nombre</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Profesion</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profesionales.each do |profesionale| %>
      <tr>
        <td><%= profesionale.rut %></td>
        <td><%= profesionale.nombre %></td>
        <td><%= profesionale.segundo_nombre %></td>
        <td><%= profesionale.apellido_paterno %></td>
        <td><%= profesionale.apellido_materno %></td>
        <td><%= profesionale.profesion %></td>
        <td><%= profesionale.descripcion %></td>
        <td><%= link_to 'Mostar', profesionale %></td>
        <td><%= link_to 'Editar', edit_profesionale_path(profesionale) %></td>
        <td><%= link_to 'Eliminar', profesionale, method: :delete, data: { confirm: 'Estas Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar Nuevo Profesional', new_profesionale_path %>
